    <declare-styleable name="IndicativeRadioGroup"><attr format="reference|color" name="indicatorDrawable"/><attr format="integer" name="indicatorMoveAnimationDuration"/><attr format="integer" name="indicatorRestoreAnimationDuration"/><attr format="reference" name="hideAnimation"/><attr format="reference" name="showAnimation"/></declare-styleable>
